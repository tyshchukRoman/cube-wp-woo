{
  "name": "cube-wp",
  "version": "0.1.0",
  "scripts": {
    "css": "postcss src/css/*.css --dir dist/css",
    "js": "esbuild src/js/main.js --bundle --splitting --format=esm --outdir=dist/js --minify",
    "js:watch": "esbuild src/js/main.js --bundle --splitting --format=esm --outdir=dist/js --watch --sourcemap",
    "clean": "rimraf dist && mkdir dist",
    "start": "concurrently \"eleventy --serve --quiet\" \"npm run css -- --watch\" \"npm run js:watch\"",
    "build": "npm run clean && eleventy && NODE_ENV=production npm run css && npm run js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "chalk": "^4.1.2",
    "clean-css": "^5.3.2",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "gsap": "^3.13.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-js": "^4.0.1",
    "slugify": "^1.6.6",
    "swiper": "^11.2.10",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "esbuild": "0.25.9",
    "postcss-nesting": "^12.1.1",
    "prettier": "^3.2.5"
  }
}
